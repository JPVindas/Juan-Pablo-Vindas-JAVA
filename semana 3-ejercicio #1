
package javaapplication13;

import javax.swing.JOptionPane;


public class JavaApplication13 {

  
    public static void main(String[] args) {
        int ancho = 10;
        int alto = 5;

        for (int i = 0; i < alto; i++) {
            for (int j = 0; j < ancho; j++) {
                System.out.print("@");
            }
            System.out.println();
        }
    }
}

    